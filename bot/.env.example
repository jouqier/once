# Telegram Bot Token (получите у @BotFather)
BOT_TOKEN=your_bot_token_here

# TMDB API Key (получите на https://www.themoviedb.org/settings/api)
TMDB_API_KEY=your_tmdb_api_key_here

# URL вашего Mini App
WEB_APP_URL=https://your-app.pages.dev

# Имя бота (без @)
BOT_USERNAME=your_bot_username

# Короткое имя приложения из BotFather
APP_SHORT_NAME=app

# Порт для webhook (опционально, для production)
PORT=3000

# Webhook URL (опционально, для production)
WEBHOOK_URL=https://your-bot-domain.com
